{
  "version": 2,
  "builds": [
    {
      "src": "api/characters.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/api/characters",
      "dest": "/api/characters.js" 
    },
    {
      "src": "/api/characters/(.*)",
      "dest": "/api/characters.js"
    }
  ]
}